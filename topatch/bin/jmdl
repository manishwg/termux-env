#!/data/data/com.termux/files/usr/bin/bash

src='/data/media/0/Android/data/com.jio.media.jiobeats/files/.assets/'
dst='/data/media/0/jmdl.tmp/'
nop=''

echo ${src}
echo ${dst}

cd ${src}

while true
do
    for i in *
    do
        if [ "${i}" != "*" ]
        then
            if [ ! -d ${dst}${i} ]
            then
			    for j in ${i}/*
			    do
			        if [ "${j}" != "${i}/*" ]
			        then
			            if [ ! -f ${dst}${j} ]
			            then
			                busybox ln ${src}${j} ${dst}${j}
			                echo ${j} -- ${i}
			            fi
			        fi
			    done
            elif [ ! -f ${dst}${i}/${i} ]
            then
                busybox ln ${src}${i} ${dst}${i}
                echo ${i}
            fi
        fi
    done
done
