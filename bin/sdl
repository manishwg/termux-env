#!/data/data/com.termux/files/usr/bin/bash

# src="/mnt/expand/d54f545e-299a-4e9e-9c06-98777b59c5fc/media/0/Android/data/com.saavn.android/files/songs_temp/"
src="/data/media/0/Android/data/com.saavn.android/files/songs_temp/"
# dst="/mnt/expand/d54f545e-299a-4e9e-9c06-98777b59c5fc/media/0/sdl.tmp/"
dst="/data/media/0/sdl.tmp/"
nop=""

echo ${src}
echo ${dst}

cd ${src}

while true
do
    for i in *
    do
        if [ "${i}" != "*" ]
        then
            if [ ! -f ${dst}${i} ]
            then
                busybox ln ${src}${i} ${dst}${i}
                echo ${i}
            fi
        fi
    done
done
