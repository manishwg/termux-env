#!/data/data/com.termux/files/usr/bin/bash

sudo youtube-dl --yes-playlist --no-part --write-sub --sub-lang en --merge-output-format mp4 -f "best[ext=mp4][height<=480]/bestvideo[ext=mp4][height<=480]+bestaudio[ext=m4a]/22" -o "/storage/emulated/0/[YTL]/%(title)s [%(height)d] {%(id)s}.%(ext)s" https://www.youtube.com/playlist?list=PLAjeEoHW9hredqsq0Ffs6kB7pvmhSoSR4 $*
